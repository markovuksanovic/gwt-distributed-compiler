<!DOCTYPE databases PUBLIC
"-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
"http://castor.exolab.org/mapping.dtd">

<mapping>
	<description>Description of the mapping</description>
	<!-- mapping for class 'AgentSettings' -->
	<class name="com.google.gwt.dist.compiler.AgentsSettings">
		<map-to xml="nodes"/>
		<field name="nodes" type="com.google.gwt.dist.impl.NodeImpl" collection="arraylist">
			<bind-xml name="node" node="element"/>
		</field>
	</class>

	<!-- mapping for class 'NodeImpl' -->
	<class name="com.google.gwt.dist.impl.NodeImpl">
		<field name="ipaddress" type="string">
			<bind-xml name="ipaddress" node="element"/>
		</field>
		<field name="port" type="integer">
			<bind-xml name="port" node="element"/>
		</field>
	</class>

</mapping>